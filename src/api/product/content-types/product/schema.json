    },
    "rating": {
      "type": "decimal",
      "max": 5
    },
    "review_count": {
      "type": "integer"
    },
    "location": {
      "type": "string"
    },
    "services_offered": {
      "type": "json"
    },
    "Industries_served": {
      "type": "json"
    },
    "project_cost": {
      "type": "string"
    },
    "retainer": {
      "type": "string"
    }
